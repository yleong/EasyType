<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="24%p"
    android:horizontalGap="1px"
    android:verticalGap="1px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="49" android:keyIcon = "@drawable/one" android:keyEdgeFlags="left"/>
        <Key android:codes="50" android:keyIcon = "@drawable/two"/>
        <Key android:codes="51" android:keyIcon = "@drawable/three"/>
        <Key android:codes="45" android:keyIcon = "@drawable/setting" android:keyEdgeFlags="right"/>
        
    </Row>
    
    <Row>
        <Key android:codes="52" android:keyIcon = "@drawable/four" android:keyEdgeFlags="left"/>
        <Key android:codes="53" android:keyIcon = "@drawable/five"/>
        <Key android:codes="54" android:keyIcon = "@drawable/six"/>
        <Key android:codes="46" android:keyIcon = "@drawable/fullstop" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="55" android:keyIcon = "@drawable/seven" android:keyEdgeFlags="left"/>
        <Key android:codes="56" android:keyIcon = "@drawable/eight"/>
        <Key android:codes="57" android:keyIcon = "@drawable/nine"/>
        <Key android:codes="-5" android:keyIcon = "@drawable/delete" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="42" android:keyIcon = "@drawable/shift" android:keyEdgeFlags="left"/>
        <Key android:codes="48" android:keyIcon = "@drawable/zero"/>
        <Key android:codes="32" android:keyIcon = "@drawable/sym_keyboard_space"/>
        <Key android:codes="10" android:keyIcon = "@drawable/sym_keyboard_return" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    